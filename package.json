{
  "name": "polymer-redux-hn",
  "flat": true,
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/PolymerLabs/polymer-redux-hn.git"
  },
  "contributors": [],
  "scripts": {
    "bundle": "node ../../Polymer/polymer-bundler/lib/bin/polymer-bundler.js --out-dir bundled --shell src/components/hn-app.js src/components/hn-app.js",
    "minify": "babel bundled --out-dir dist",
    "copy": "cp -r images index.html manifest.json dist",
    "sw": "workbox generateSW workbox-config.js",
    "copy-polyfills": "cp -r node_modules/@webcomponents/webcomponentsjs .",
    "clean": "rm -rf bundled dist webcomponentsjs",
    "build": "npm run clean && npm run bundle && npm run minify && npm run copy && npm run sw && npm run copy-polyfills"
  },
  "dependencies": {
    "@0xcda7a/redux-es6": "^3.7.2",
    "@polymer/lit-element": "^0.2.2",
    "lit-html": "^0.8.0",
    "pwa-helpers": "PolymerLabs/pwa-helpers",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-minify": "^0.2.0",
    "workbox-cli": "^3.0.0"
  }
}
